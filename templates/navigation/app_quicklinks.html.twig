{% set class = class ?? '' %}

{% if is_granted('IS_AUTHENTICATED_FULLY') and unchained.navigation.show_quicklinks %}
    <a class="{{ class }} plus" href="{{ url('admin_edit', {app: appId, category: application.config.category.categoryId}) }}"
       title="{{ ('action.entity.create.verbose')|trans({ verbose: ('entity.verbose')|trans([], application.domain) }, application.domain) }}">
        <i class="fa-solid fa-plus"></i>
    </a>
{% endif %}